<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xix.cleanMvpArchitecture"
    >

  <uses-permission android:name="android.permission.INTERNET"/>
  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:name="com.xix.cleanMvpArchitecture.ProjectApplication"
      android:theme="@style/AppTheme"
      >
    <meta-data
        android:name="io.fabric.ApiKey"
        android:value="06e0112d7da1f4b813754e230e58e67bcef9660a"
        />


    <activity
        android:name="com.xix.cleanMvpArchitecture.presentation.screens.navigation.NavigationActivity"
        android:launchMode="singleTop"
        android:screenOrientation="portrait"
        android:theme="@style/AppThemeFullScreen"
        >
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>


  </application>
</manifest>